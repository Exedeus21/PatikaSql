--3.Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.
Update employee
set name= 'Ali',
	birthday='1995-12-12',
	email='Ali@kocan.com'
where id=3;

Update employee
set name= 'Veli',
	birthday='1945-11-10',
	email='Veli@kocan.com'
where id=4;

Update employee
set name= 'Ahmet',
	birthday='1973-3-12',
	email='Ahmet@hasan.com'
where id=5;

Update employee
set name= 'Sena',
	birthday='1982-3-3',
	email='Sena@kaçan.com'
where id=7;

Update employee
set name= 'Deniz',
	birthday='1998-1-31',
	email='Deniz@orhan.com'
where id=1;

--4.Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee WHERE id>10 and id<= 15;


